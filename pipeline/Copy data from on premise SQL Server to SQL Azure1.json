				"inputs": [
					{
						"referenceName": "GCUI01_Retail_sales",
						"type": "DatasetReference"
					}
				],
				"outputs": [
					{
						"referenceName": "ADLS_Retail_sales",
						"type": "DatasetReference"
					}
