				"inputs": [
					{
						"referenceName": "SourceDataset_JSON",
						"type": "DatasetReference"
					}
				],
				"outputs": [
					{
						"referenceName": "ADLS_Retail_sales",
						"type": "DatasetReference"
					}
				]
			},
